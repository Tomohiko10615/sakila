package com.tracen.dvdrental.entity;

public enum ratings {
	G("G"), PG("PG"), PG13("PG-13"), R("R"), NC17("NC-17");
	
	private String rating;

	ratings(String rating) {
		this.setRating(rating);
	}

	public String getRating() {
		return rating;
	}

	public void setRating(String rating) {
		this.rating = rating;
	}
}
